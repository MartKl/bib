@article{Berinde2010a,
abstract = {We consider the approximate sparse recovery problem, where the goal is to (approximately) recover a high-dimensional vector x from its lower-dimensional sketch Ax. A popular way of performing this recovery is by finding x such that Ax = Ax , and x 1 is minimal. It is known that this approach works if A is a random dense matrix, chosen from a proper distribution. In this paper, we investigate this procedure for the case where A is binary and very sparse. We show that, both in theory and in practice, sparse matrices are essentially as good as the dense ones. At the same time, sparse binary matrices provide additional benefits, such as reduced encoding and decoding time.},
author = {Berinde, R and Indyk, P},
doi = {10.1109/JPROC.2010.2045092},
file = {:home/jungp/Papers/CompressedSensing/L1-RIP/Berinde-Sparse{\_}recovery{\_}using{\_}sparse{\_}random{\_}matrices.pdf:pdf},
