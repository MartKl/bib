@article{Walk2010,
abstract = {In this contribution we present a novel method for constructing orthogonal$\backslash$npulses for UWB impulse radio transmission under the FCC spectral$\backslash$nmask constraint. In contrast to previous work we combine a convex$\backslash$nformulation of the spectral design with L{\{}{\"{o}}{\}}wdin's orthogonalization$\backslash$nmethod $\backslash$cite{\{}Low50{\}}, which delivers a shift--orthogonal basis optimally$\backslash$nclose (in energy) to the initial pulse, which generates (in a stable$\backslash$nway) the shift--invariant space. The convex formulation of the spectral$\backslash$ndesign is achieved by approximating the FCC mask with a finite--order$\backslash$nfilter matched to Gaussian monocycles as input. The output pulse$\backslash$nthen has high energy concentration in the passband (NESP value).$\backslash$nUsing L{\{}{\"{o}}{\}}wdin's orthogonalization we compute the corresponding$\backslash$nshift--orthogonal pulse. We show that our approach is able to generate$\backslash$nfor finitely many shifts, orthogonal equal energy pulses with nearly$\backslash$nthe same NESP value. Furthermore, we show that the orthogonalization$\backslash$nprocedure can be well approximated using the Zak transform allowing$\backslash$nfor an efficient implementation with the discrete Fourier transform.$\backslash$nSurprisingly, we could observe, that for certain parameters, this$\backslash$napproximation yields almost the same performance as the exact L{\{}{\"{o}}{\}}wdin$\backslash$nmethod.},
author = {Walk, Philipp and Jung, P and Timmermann, Jens},
doi = {10.1109/WCNC.2010.5506503},
isbn = {9781424463985},
issn = {1525-3511},
journal = {{\{}Ieee{\}} {\{}Wcnc{\}}},
number = {1},
title = {{L{\{}{\"{o}}{\}}wdin Transform on {\{}FCC{\}} Optimized {\{}UWB{\}} Pulses}},
year = {2010}
}
