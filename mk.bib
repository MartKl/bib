	keywords = {Quantum Physics, Ultracold gases},
	pages = {521--525},
	file = {Full Text PDF:/home/dhangleiter/.mozilla/firefox/2jx0egeo.default/zotero/storage/NC7RJ4H4/Yan et al. - 2013 - Observation of dipolar spin-exchange interactions .pdf:application/pdf;Snapshot:/home/dhangleiter/.mozilla/firefox/2jx0egeo.default/zotero/storage/9ES66RHZ/nature12483.html:text/html}
}
