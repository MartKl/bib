@article{nakata_decoupling_2015,
	title = {Decoupling with random diagonal unitaries},
	url = {http://arxiv.org/abs/1509.05155},
	abstract = {We investigate decoupling, one of the most important primitives in quantum Shannon theory, by replacing the uniformly distributed random unitaries commonly used to achieve the protocol, with alternate applications of random unitaries diagonal in the Pauli-\$Z\$ and -\$X\$ bases. This strategy was recently shown to achieve an approximate unitary \$2\$-design after a number of repetitions of the process, which implies that the strategy gradually achieves decoupling. Here, we prove that even fewer repetitions of the process suffice to achieve decoupling, showing that unitary \$2\$-designs with rather imprecise approximations can achieve decoupling at the same rate as that with the uniform ones. Based on this, we provide a simple quantum circuit achieving decoupling, which can be partitioned into a constant number of commuting parts and may have a practical merit, and a random many-body Hamiltonian, of which dynamics achieves decoupling in a constant time even if the system consists of a large number of particles. We also present applications of our decoupling theorem to coherent state merging and relative thermalisation.},
	urldate = {2017-05-12},
	journal = {arXiv:1509.05155 [quant-ph]},
	author = {Nakata, Yoshifumi and Hirche, Christoph and Morgan, Ciara and Winter, Andreas},
	month = {09},
