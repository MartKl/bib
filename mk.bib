@article{Spagnolo2013,
abstract = {A boson sampling device is a specialised quantum computer that solves a problem which is strongly believed to be computationally hard for classical computers. Recently a number of small-scale implementations have been reported, all based on multi-photon interference in multimode interferometers. In the hard-to-simulate regime, even validating the device's functioning may pose a problem . In a recent paper, Gogolin et al. showed that so-called symmetric algorithms would be unable to distinguish the experimental distribution from the trivial, uniform distribution. Here we report new boson sampling experiments on larger photonic chips, and analyse the data using a scalable statistical test recently proposed by Aaronson and Arkhipov. We show the test successfully validates small experimental data samples against the hypothesis that they are uniformly distributed. We also show how to discriminate data arising from either indistinguishable or distinguishable photons. Our results pave the way towards larger boson sampling experiments whose functioning, despite being non-trivial to simulate, can be certified against alternative hypotheses.},
archivePrefix = {arXiv},
arxivId = {1311.1622},
author = {Spagnolo, Nicol{\`{o}} and Vitelli, Chiara and Bentivegna, Marco and Brod, Daniel J and Crespi, Andrea and Flamini, Fulvio and Giacomini, Sandro and Milani, Giorgio and Ramponi, Roberta and Mataloni, Paolo and Osellame, Roberto and Galv{\~{a}}o, Ernesto F. and Sciarrino, Fabio},
doi = {10.1038/nphoton.2014.135},
eprint = {1311.1622},
issn = {1749-4885},
journal = {Nature Photonics},
month = 06,
number = {8},
pages = {615--620},
title = {{Experimental validation of photonic boson sampling}},
url = {http://arxiv.org/abs/1311.1622 http://dx.doi.org/10.1038/nphoton.2014.135 http://www.nature.com/doifinder/10.1038/nphoton.2014.135},
volume = {8},
year = {2014}
}
