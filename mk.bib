@article{Brian2014,
abstract = {{\$}\backslashmathbb{\{}N{\}}{\^{}}* = \backslashbeta\backslashmathbb{\{}N{\}} \backslashsetminus \backslashmathbb{\{}N{\}}{\$} has a canonical dynamical structure provided by the shift map, the unique continuous extension to {\$}\backslashbeta\backslashmathbb{\{}N{\}}{\$} of the map {\$}n \backslashmapsto n+1{\$} on {\$}\backslashmathbb{\{}N{\}}{\$}. Here we investigate the question of what dynamical systems can be written as quotients of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$}. We prove that a dynamical system is a quotient of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$} if and only if it is isomorphic to the {\$}\backslashomega{\$}-limit set of some point in some larger system. This provides a full external characterization of the quotients of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$}. We also prove, assuming MA{\$}{\_}{\{}\backslashsigma\backslashtext{\{}-centered{\}}{\}}(\backslashkappa){\$}, that a dynamical system of weight {\$}\backslashkappa{\$} is a quotient of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$} if and only if it is chain transitive. This provides a consistent partial internal characterization of the quotients of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$}, and a full internal characterization for metrizable systems.},
archivePrefix = {arXiv},
arxivId = {1501.0157},
author = {Brian, William R.},
eprint = {1501.0157},
month = 12,
number = {December},
title = {{Quotients of {\$}\backslashmathbb{\{}N{\}}{\^{}}*{\$}, {\$}\backslashomega{\$}-limit sets, and chain transitivity}},
