@article{takeuchi_ancilla-driven_2016,
	title = {Ancilla-driven instantaneous quantum polynomial time circuit for quantum supremacy},
	url = {http://arxiv.org/abs/1611.00510},
	abstract = {Instantaneous quantum polynomial time (IQP) is a model of (probably) non-universal quantum computation. Since it has been proven that IQP circuits are unlikely to be simulated classically up to a multiplicative error and an error in l1 norm, IQP is considered as one of the promising classes that demonstrates quantum supremacy. Although IQP circuits can be realized more easily than a universal quantum computer, demonstrating quantum supremacy is still difficult. It is therefore desired to find subclasses of IQP that are easy to implement. In this paper, by imposing some restrictions on IQP, we propose ancilla-driven IQP (ADIQP) as the subclass of commuting quantum computation suitable for many experimental settings. We show that even though ADIQP circuits are strictly weaker than IQP circuits in a sense, they are also hard to simulate classically up to a multiplicative error and an error in l1 norm. Moreover, the properties of ADIQP make it easy to investigate the verifiability of ADIQP circuits and the difficulties in realizing ADIQP circuits.},
	urldate = {2016-11-04},
	journal = {arXiv:1611.00510 [quant-ph]},
	author = {Takeuchi, Yuki and Takahashi, Yasuhiro},
	month = 11,
	year = {2016},
	note = {arXiv: 1611.00510},
	keywords = {Quantum Physics, Computer Science - Computational Complexity},
	file = {arXiv\:1611.00510 PDF:/home/dhangleiter/.mozilla/firefox/2jx0egeo.default/zotero/storage/8TAHVECF/Takeuchi and Takahashi - 2016 - Ancilla-driven instantaneous quantum polynomial ti.pdf:application/pdf;arXiv.org Snapshot:/home/dhangleiter/.mozilla/firefox/2jx0egeo.default/zotero/storage/69W54DQF/1611.html:text/html}
}
@article{de_luca_ergodicity_2013,
